<h1>Transactions for <%= user.username %></h1>
<table class="table table-bordered table-striped">

    <thead>
        <tr>
            <th>description</th>
            <th>amount</th>
            <th>category</th>
            <th>date</th>
            <th>delete</th>
            <th>edit</th>

        </tr>
    </thead>
    <tbody>
        <% results.map(item => {%>
            <tr>
               <td> <%= item._id.description %> </td>
               <td><%= item._id.amount %></td>
               <td><%= item.category %></td>
               <td><%= item.date %></td>
               <td><a href="/todo/delete_transaction/<%=item._id %>">
                <i class="bi bi-trash"></i>
              </a></td>
              <td><a href="/todo/edit_transaction/<%=item._id %>">
                <i class="bi bi-pencil-square"></i>
              </a></td>
            </tr>
            <% }) %>
    </tbody>
</table>