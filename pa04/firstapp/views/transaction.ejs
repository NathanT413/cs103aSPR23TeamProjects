<h1>Summary of all Transactions</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>description</th>
            <th>amount</th>
            <th>category</th>
            <th>date</th>
        </tr>
    </thead>
    <tbody>
        <% results.map(item => {%>
            <tr>
               <td> <%= item._id.description %> </td>
               <td><%= item._id.amount %></td>
               <td><%= item.category %></td>
               <td><%= item.date %></td>
            </tr>
            <% }) %>
    </tbody>
</table>